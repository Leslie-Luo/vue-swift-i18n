{
  "name": "vue-swift-i18n",
  "displayName": "vue-swift-i18n",
  "description": "swift i18n plugin for vue developer",
  "publisher": "RichieChoo",
  "keywords": [
    "vue",
    "i18n",
    "quick",
    "fast",
    "swift"
  ],
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.36.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "images/icon.png",
  "activationEvents": [
    "onCommand:extension.vueSwiftI18n.flatJson",
    "onCommand:extension.vueSwiftI18n.swiftI18n"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.vueSwiftI18n.flatJson",
        "title": "Flat Json"
      },
      {
        "command": "extension.vueSwiftI18n.swiftI18n",
        "title": "Swift I18n ",
        "icon":  {
          "light": "./images/icon-light.svg",
          "dark": "./images/icon-dark.svg"
        }
      }
    ],
    "keybindings": [
			{
				"command": "extension.vueSwiftI18n.swiftI18n",
				"key": "ctrl+f1",
				"mac": "cmd+f1",
				"when": "editorTextFocus"
			}
		],
    "menus": {
      "editor/context": [
        {
          "when": "editorFocus && resourceLangId == json",
          "command": "extension.vueSwiftI18n.flatJson",
          "group": "navigation"
        },
        {
          "when": "editorFocus && resourceLangId == vue",
          "command": "extension.vueSwiftI18n.swiftI18n",
          "group": "1_modification"
        },
        {
          "when": "editorFocus && resourceLangId == javascript",
          "command": "extension.vueSwiftI18n.swiftI18n",
          "group": "1_modification"
        }
      ],
      "editor/title/context": [
        {
          "when": "resourceLangId == json",
          "command": "extension.vueSwiftI18n.flatJson",
          "group": "navigation"
        },
        {
          "when": "resourceLangId == vue",
          "command": "extension.vueSwiftI18n.swiftI18n",
          "group": "1_modification"
        }
      ],
      "explorer/context": [
        {
          "command": "extension.vueSwiftI18n.flatJson",
          "when": "explorerResourceIsFolder",
          "group": "navigation"
        },
        {
          "command": "extension.vueSwiftI18n.swiftI18n",
          "when": "explorerResourceIsFolder",
          "group": "1_modification"
        },
        {
          "command": "extension.vueSwiftI18n.flatJson",
          "when": "resourceLangId == json",
          "group": "navigation"
        },
        {
          "command": "extension.vueSwiftI18n.swiftI18n",
          "when": "resourceLangId == vue",
          "group": "1_modification"
        },
        {
          "command": "extension.vueSwiftI18n.swiftI18n",
          "when": "resourceLangId == javascript",
          "group": "1_modification"
        }
      ]
    },
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "vue",
        "path": "./snippets/vue.json"
      }
    ]
  },
  "scripts": {
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "ypes/glob": "^7.1.1",
    "ypes/mocha": "^5.2.6",
    "ypes/node": "^10.12.21",
    "ypes/vscode": "^1.36.0",
    "eslint": "^5.13.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "typescript": "^3.3.1",
    "vscode-test": "^1.0.0-next.0"
  },
  "dependencies": {
    "flat": "^4.1.0"
  },
  "bugs": {
    "url": "https://github.com/RichieChoo/vue-swift-i18n/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RichieChoo/vue-swift-i18n"
  },
  "homepage": "https://github.com/RichieChoo/vue-swift-i18n/blob/master/README.md"
}
